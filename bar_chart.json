{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 500,
  "data": {
    "url": "https://raw.githubusercontent.com/Abdallah-92/US_GDP-DataVis/main/Income.csv"
  },
  "height": {"step": 10},
  "mark": {"type": "bar", "color": "red"},
  "encoding": {
    "y": {"field": "State", "type": "nominal", "axis": {"labelAngle": 0}},
    "x": {"field": "Income", "type": "quantitative"},
    "tooltip": [
      {"field": "State", "type": "ordinal"},
      {"field": "Income", "type": "quantitative", "format": ","}
    ]
  },
  "config": {}
}